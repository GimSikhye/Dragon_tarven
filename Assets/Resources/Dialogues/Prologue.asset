  m_Script: {fileID: 11500000, guid: a5567b85d78bee0459b5b89e85a8aebc, type: 3}
  m_Name: Prologue
  m_EditorClassIdentifier: 
  lines:
  - speaker: {fileID: 11400000, guid: 93f7f782c94526846b27620cf85a2a0e, type: 2}
    isNarration: 0
    isInnerFeelings: 0
    dialogueTexts:
    - text: "\uC5B4\uB290\uB367 \uB098\uC774\uB3C4 25 \uAC00\uAE4C\uC774 \uB418\uC5C8\uB2E4."
      sfx: {fileID: 0}
      image: {fileID: 0}
    - text: "\uB300\uD559\uAD50 \uC804\uACF5\uC740 \uC0AC\uC9C4\uD559\uACFC \uCABD\uC774\uC9C0\uB9CC
        \uC804\uD600 \uC0C1\uAD00\uC5C6\uB294 \uCE74\uD398\uC54C\uBC14\uB97C \uD558\uACE0
        \uC788\uB2E4."
      sfx: {fileID: 0}
      image: {fileID: 0}
  - speaker: {fileID: 11400000, guid: 93f7f782c94526846b27620cf85a2a0e, type: 2}
    isNarration: 0
    isInnerFeelings: 1
    dialogueTexts:
    - text: "\uACFC\uB3D9\uAE30\uB4E4\uC774 \uAC19\uC774 \uC804\uACFC\uB97C \uC81C\uC548\uD588\uC744
        \uB54C, \uB098\uB3C4 \uCDE8\uC5C5 \uC798\uB418\uB294 \uD559\uACFC\uB85C \uAC19\uC774
        \uAC14\uC5B4\uC57C \uD588\uB098?"
      sfx: {fileID: 0}
      image: {fileID: 0}
  - speaker: {fileID: 11400000, guid: 93f7f782c94526846b27620cf85a2a0e, type: 2}
    isNarration: 1
    isInnerFeelings: 0
    dialogueTexts:
    - text: "\uC9C0\uAE08 \uD6C4\uD68C\uD574\uBD24\uC790 \uBC14\uB00C\uB294 \uAC83\uC740
        \uC5C6\uAE34 \uD558\uC9C0\uB9CC \uC774\uB530\uAE08 \uC774\uB7F0 \uC0DD\uAC01\uC774
        \uB4DC\uB294\uAC74 \uC5B4\uCA54 \uC218 \uC5C6\uB2E4."
      sfx: {fileID: 0}
      image: {fileID: 0}
    - text: "\uC8FC\uBCC0 \uCE5C\uAD6C\uB4E4\uC740 \uAC01\uC790 \uC804\uACF5\uC744
        \uC0B4\uB824 \uD604\uC5C5\uC5D0\uC11C \uC77C\uD558\uACE0 \uC788\uAC74\uB9CC
        \uB098\uB9CC \uC544\uC9C1 \uCDE8\uC5C5\uC744 \uD558\uC9C0 \uBABB\uD574 \uC54C\uBC14\uC0DD
        \uC2E0\uC138\uB2E4."
      sfx: {fileID: 0}
      image: {fileID: 0}
    - text: "[\uCE5C\uAD6C\uB4E4\uC758 \uC778\uC2A4\uD0C0\uADF8\uB7A8 \uC0AC\uC9C4\uB4E4]
        (\uC2B9\uC9C4, \uD68C\uC2DD \uC0AC\uC9C4, \uC601\uD654 \uAC10\uB3C5\uC0AC\uC9C4)"
      sfx: {fileID: 0}
      image: {fileID: 0}
    - text: "SNS\uC5D0 \uB4E4\uC5B4\uAC00\uBA74 \uC774\uB7F0\uC0AC\uC9C4\uB4E4 \uBFD0\uC774\uB2E4.
        \uC694\uC998 SNS\uB3C4 \uC774\uB798\uC11C \uC548\uBCF8\uC9C0 \uAF64 \uB418\uC5C8\uB2E4.\r\n"
      sfx: {fileID: 0}
      image: {fileID: 0}
    - text: "\uC694\uC998 \uCE5C\uAD6C\uB4E4\uACFC\uC758 \uC548\uBD80\uC804\uD654\uB3C4
        \uB2E4 \uB04A\uACE0 \uC0B4\uACE0 \uC788\uB2E4."
      sfx: {fileID: 0}
      image: {fileID: 0}
  - speaker: {fileID: 11400000, guid: 93f7f782c94526846b27620cf85a2a0e, type: 2}
    isNarration: 0
    isInnerFeelings: 1
    dialogueTexts:
    - text: "\uCE74\uD398 \uC54C\uBC14\uB3C4 2\uB144 \uC815\uB3C4 \uB418\uC5C8\uB294\uB370
        \uC774\uAC78 \uACBD\uB825\uC73C\uB85C \uCCD0\uC8FC\uC9C0\uB294 \uC54A\uACA0\uC9C0?"
      sfx: {fileID: 0}
      image: {fileID: 0}
  - speaker: {fileID: 11400000, guid: 93f7f782c94526846b27620cf85a2a0e, type: 2}
    isNarration: 1
    isInnerFeelings: 0
    dialogueTexts:
    - text: "\uB300\uCCB4 \uC5B4\uB514\uC11C\uBD80\uD130 \uAF2C\uC778\uAC78\uAE4C..."
      sfx: {fileID: 0}
      image: {fileID: 0}
    - text: "[\uB538\uB791]"
      sfx: {fileID: 0}
      image: {fileID: 0}
    - text: "\uD559\uC0DD \uC190\uB2D8: \uC544\uC774\uC2A4 \uC544\uBA54\uB9AC\uCE74\uB178
        \uC5BC\uC74C \uB9CE\uC774 \uC8FC\uC138\uC694!"
      sfx: {fileID: 0}
      image: {fileID: 0}
  - speaker: {fileID: 11400000, guid: 93f7f782c94526846b27620cf85a2a0e, type: 2}
    isNarration: 0
    isInnerFeelings: 0
    dialogueTexts:
    - text: "\uB124. \uC8FC\uBB38 \uBC1B\uC558\uC2B5\uB2C8\uB2E4. \uC7A0\uC2DC\uB9CC
        \uAE30\uB2E4\uB824\uC8FC\uC138\uC694."
      sfx: {fileID: 0}
      image: {fileID: 0}
  - speaker: {fileID: 11400000, guid: 93f7f782c94526846b27620cf85a2a0e, type: 2}
    isNarration: 1
    isInnerFeelings: 0
    dialogueTexts:
    - text: "-\uD29C\uD1A0\uB9AC\uC5BC! \uC190\uB2D8\uC758 \uC8FC\uBB38\uC744 \uBC1B\uC544
        \uCEE4\uD53C\uB97C \uB9CC\uB4DC\uC138\uC694! \uC190\uB2D8\uC758 \uB9CC\uC871\uB3C4\uC5D0
        \uB530\uB77C \uCD94\uAC00 \uD301\uACFC \uD3C9\uD310\uC744 \uC5BB\uC744 \uC218
        \uC788\uC2B5\uB2C8\uB2E4!-"
      sfx: {fileID: 0}
      image: {fileID: 0}
